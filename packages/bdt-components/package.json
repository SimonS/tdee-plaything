{
  "name": "@tdee/bdt-components",
  "version": "1.0.0",
  "description": "Component Library for BreakfastDinnerTea common assets",
  "repository": "https://github.com/SimonS/tdee-plaything",
  "license": "MIT",
  "private": true,
  "scripts": {
    "storybook": "storybook dev -c .storybook",
    "build-storybook": "storybook build -c .storybook -o ../bdt-astro/dist/storybook",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@storybook/addon-actions": "^7.4.0",
    "@storybook/addon-controls": "^7.4.0",
    "@storybook/addon-docs": "^6.5.10",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/react": "^7.4.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.4",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "assert-browserify": "^2.0.0",
    "babel-jest": "^29.6.4",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass-loader": "^13.3.2",
    "storybook": "^7.4.0",
    "style-loader": "^3.3.1",
    "typescript": "^5.2.2",
    "webpack": "^5.65.0"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "webpack": "*"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!nanostores|@nanostores|@nivo|d3-color).*/"
    ]
  },
  "dependencies": {
    "@nanostores/react": "^0.7.1",
    "@nivo/calendar": "0.83.0",
    "@storybook/react-webpack5": "^7.4.0",
    "@testing-library/jest-dom": "^6.1.3",
    "css-loader": "^6.5.1",
    "jest-environment-jsdom": "^29.6.4",
    "nanostores": "^0.9.3",
    "webpack": "^5.65.0"
  }
}
