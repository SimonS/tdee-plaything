{
  "name": "@tdee/bdt-components",
  "version": "1.0.0",
  "description": "Component Library for BreakfastDinnerTea common assets",
  "repository": "https://github.com/SimonS/tdee-plaything",
  "license": "MIT",
  "private": true,
  "scripts": {
    "storybook": "storybook dev -c .storybook",
    "build-storybook": "storybook build -c .storybook -o ../bdt-astro/dist/storybook",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@storybook/addon-actions": "^8.6.7",
    "@storybook/addon-controls": "^8.6.7",
    "@storybook/addon-docs": "^8.6.7",
    "@storybook/addon-links": "^8.6.7",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.5",
    "@storybook/react": "^8.6.7",
    "@storybook/react-webpack5": "^8.6.7",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/recharts": "^1.8.24",
    "assert-browserify": "^2.0.0",
    "babel-jest": "^29.6.4",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.4",
    "path-browserify": "^1.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "sass-loader": "^16.0.5",
    "storybook": "^8.6.7",
    "style-loader": "^4.0.0",
    "typescript": "^5.2.2",
    "webpack": "^5.65.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "webpack": "*"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!nanostores|@nanostores|@nivo|d3-*|internmap|delaunator|robust-predicates).*/"
    ]
  },
  "dependencies": {
    "@nanostores/react": "^0.8.4",
    "@nivo/calendar": "^0.88.0",
    "@nivo/colors": "^0.88.0",
    "@nivo/core": "^0.88.0",
    "date-fns": "^2.30.0",
    "jest-environment-jsdom": "^29.6.4",
    "nanostores": "^0.11.4",
    "react-day-picker": "^9.6.2",
    "recharts": "^2.8.0"
  }
}
